func sum <int a, int b> -> int {
  return a + 3*b.
}

exec {
  const sum1 := <int a, int b> -> int {return 1 * sum(a, b).}. 
  const sum2 := <int a, int b> -> int {return 2 * sum(a, b).}. 
  const sum3 := <int a, int b> -> int {return 3 * sum(a, b).}. 
  const sum4 := <int a, int b> -> int {return 4 * sum(a, b).}. 

  const arrayLambda := [sum1, sum2, sum3, sum4].

  var nice := arrayLambda[3](23, 33).
  print nice.
}